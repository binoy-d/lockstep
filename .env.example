# Required for docker-compose production-mode backend startup
# Generate with: openssl rand -hex 32
API_SESSION_SECRET=replace-with-64-char-random-hex

# Public origin served by web and used for CORS/share links
PUBLIC_ORIGIN=https://lockstep.binoy.co

# Optional admin bootstrap account on startup.
# Leave ADMIN_PASSWORD empty to disable bootstrap.
ADMIN_USERNAME=admin
ADMIN_PLAYER_NAME=admin
ADMIN_PASSWORD=
